{
   "version": 2,
   "buildCommand": "npm run build",
   "outputDirectory": "dist",
   "framework": "vite",
   "rewrites": [
      {
         "source": "/(.*)",
         "destination": "/index.html"
      }
   ],
   "headers": [
      {
         "source": "/(.*)\\.js",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "public, max-age=31536000, immutable"
            }
         ]
      },
      {
         "source": "/index.html",
         "headers": [
            {
               "key": "Cache-Control",
               "value": "no-cache"
            }
         ]
      }
   ]
}